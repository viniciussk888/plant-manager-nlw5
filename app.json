{
  "expo": {
    "name": "PlantManager",
    "slug": "plantmanager",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.birobirobiro.plantmanager"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": "Manage your plants",
    "githubUrl": "https://github.com/birobirobiro/nlw-05-plantmanager"
  }
}
